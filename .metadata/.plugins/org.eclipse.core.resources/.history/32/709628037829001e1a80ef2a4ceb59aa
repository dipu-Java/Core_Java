package com.practise.SA;

public class Anagram {

	public static void main(String[] args) {
     String s="madam";
     String s1="mamad";
     for(int i=0;i<s.length();i++) {
    	 char c=s.charAt(i);
    	 if(s1.indexOf(c)==-1) {
    		 
    		 
    	 }else {
    		 System.out.println("anagram");
    		 return;
    	 }
    	 
     }
     System.out.println("not a anagram");
	}

}
