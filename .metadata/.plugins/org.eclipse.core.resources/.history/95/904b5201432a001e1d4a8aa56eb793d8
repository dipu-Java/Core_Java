package com.practise.SA;

public class Anagram {

	public final static void main(String[] args) {
     String s="madam";
     String s1="mamad";
     for(int i=0;i<s.length();i++) {
    	 if(s.length()!=s1.length()) {
    		 System.out.println("not a anagram.....");
    		 return;
    	 }
    	 char c=s.charAt(i);
    	 if(s1.indexOf(c)!=-1) {
    		  continue;   		 
    	 }else {
    		 System.out.println("not a anagram");
    		 return;
    	 }   	 
     }
     System.out.println("anagram");
	}

}
