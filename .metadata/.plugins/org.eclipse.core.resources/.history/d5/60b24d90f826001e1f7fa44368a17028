package com.practise.SA;

public class Demo8 {
  public void run() {
	  for(int i=1;i<=5;i++) {
		  try {
			  Thread.sleep(2000);
		  }catch(Exception e) {
			  System.out.println(e);
		  }
		  System.out.println(i);
	  }
  }
	public static void main(String[] args)throws Exception {
    for(int i=1;i<=5;i++) {
//    	Thread.sleep(2000);
    	System.out.println(i);
    }
	}

}
