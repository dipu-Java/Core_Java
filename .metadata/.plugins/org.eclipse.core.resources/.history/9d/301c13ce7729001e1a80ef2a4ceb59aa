package com.practise.SA;

public class Anagram {

	public static void main(String[] args) {
     String s="madam";
     String s1="mamad";
     for(int i=0;i<s.length();i++) {
    	 if(s.indexOf(s1)==-1) {
    		 System.out.println("not a anagram");
    	 }else {
    		 System.out.println("anagram");
    	 }
     }
	}

}
