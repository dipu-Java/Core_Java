package com.practise.SA;

public class Demo11 {
	public static void main(String[] args) {
		String s = "java is easy";
		String s1 = "";
		int count = 0;
		int i = 0;
		int j=0;
		int len = s.length();
		char[] c = s.toCharArray();
		while (len > i) {
			while (len > i && c[i] != ' ') {
				i++;
			}
			int k=i-1;
			String temp="";
			while(k>j) {
				temp+=c[k];
				k--;
			}
			s1=temp+" ";
			i=i+1;
			j=i;
		}
		System.out.println(s1);
	}
}
