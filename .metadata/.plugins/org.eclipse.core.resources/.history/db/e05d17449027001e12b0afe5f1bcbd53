package com.practise.SA;

public class Demo9 {

	public static void main(String[] args) {
		String s = "madam";
		char temp = ' ';
		int count = 0;
		char[] c=s.toCharArray();
		for (int i = 0; i < s.length(); i++) {
			for (int j = i+1; j < s.length(); j++) {
				
				if (c[i]==c[j]) {
					count++;
					c[i]=' ';
				}

				
			}
		}
		System.out.println(c + " " + count);
	}
}
