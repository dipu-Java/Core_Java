package com.practise.SA;

public class Demo9 {

	public static void main(String[] args) {
		String s = "madam";
		int temp = 0;
		int count = 0;
		char[] c=s.toCharArray();
		for (int i = 0; i < s.length(); i++) {
			 count = 0;
			for (int j = i+1; j < s.length(); j++) {
				
				if (c[i]==c[j]) {
					count++;
				}	
				if (c[i]==c[j]) {
					temp=count;
				}				
			}		
		}
		System.out.println(c + " " + temp);
	}
}
