		String s="a quick brown fox jumps over the lazy dog";
		for(char c='a';c<='z';c++) {
			if(s.indexOf(c)!=-1) {
				System.out.println("panagram");
			}else {
				System.out.println("not a panagram");
			}
		}
	
	}
